{
  "expo": {
    "name": "Muscle Recovery",
    "slug": "muscle-recovery-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "./assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musclerecovery.app",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ],
        "NSUserTrackingUsageDescription": "This app does not track you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "icon": "./assets/icon.png",
      "package": "com.musclerecovery.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 9,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "b18ebdc9-ca4d-42ad-8151-9ca0dddea9f1"
      }
    },
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "description": "Track your muscle recovery after workouts with personalized timers and cloud synchronization.",
    "githubUrl": "https://github.com/RedXP2/capstone-2"
  }
}
